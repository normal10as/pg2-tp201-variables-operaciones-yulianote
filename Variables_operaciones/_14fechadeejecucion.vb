Module _14fechadeejecucion
    Sub Main()
        Dim hoy As Date
        hoy = Now
        Console.WriteLine("La fecha del ordenador es = " & hoy)
        Console.WriteLine("El dia es = " & hoy.Day)
        Console.WriteLine("El mes es = " & hoy.Month)
        Console.WriteLine("La hora es = " & hoy.Hour)
        Console.WriteLine("El minuto es =" & hoy.Minute)
    End Sub
End Module


